{{#each logs key='id' as |log|}}
    {{review-log-feed-entry log=log}}
{{/each}}
{{#if loadingPage}}
    <div class='loading'>
        <i class='fa fa-spinner fa-pulse fa-3x' aria-hidden='true'></i>
    </div>
{{else if moreLogs}}
    <div class='more-logs'>
        <a role='button' {{action 'nextPage'}}>See more</a>
    </div>
{{/if}}
